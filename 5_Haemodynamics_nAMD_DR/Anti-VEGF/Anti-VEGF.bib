	note = {Number: 8
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {aflibercept, Age-Related Macular Degeneration, bevacizumab, inhibitors, intravitreal, pharmacokinetics, ranibizumab, vascular endothelial growth factor/antagonists \&amp},
	pages = {365},
